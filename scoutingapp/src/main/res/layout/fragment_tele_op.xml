<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/telendgame_sv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    tools:context=".ui.dataentry.TeleOpFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="433dp"
            android:layout_below="@id/teleop_continue"
            android:src="@drawable/map" />

        <LinearLayout
            android:id="@+id/teleop_layout_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/teleop_text"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/teleop_layout_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/foul_tele_text"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:text="Fouls"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/foul_teleop"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/teleop_layout_2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/teleop_level_one_text"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:text="Level 1"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_level_1"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_weight="1">

                </org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet>

                <TextView
                    android:id="@+id/teleop_zone_text_4"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Zone 4"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_zone_4"
                    android:layout_width="220dp"
                    android:layout_height="80dp"
                    android:layout_weight="1">

                </org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/teleop_layout_3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/teleop_zone_two_text"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:text="Zone 2"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_zone_2"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_weight="1">

                </org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet>

                <TextView
                    android:id="@+id/teleop_zone_text_net"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Net"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_zone_net"
                    android:layout_width="220dp"
                    android:layout_height="80dp"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/teleop_layout_4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/teleop_zone_three_text"
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:text="Zone 3"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_zone_3"
                    android:layout_width="220dp"
                    android:layout_height="50dp"
                    android:layout_weight="1">

                </org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet>

                <TextView
                    android:id="@+id/teleop_zone_text_missed"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Shots Missed"
                    android:textSize="24sp" />

                <org.usfirst.frc.team25.scouting.ui.views.ButtonIncDecSet
                    android:id="@+id/teleop_zone_missed"
                    android:layout_width="220dp"
                    android:layout_height="80dp"
                    android:layout_weight="1" />

            </LinearLayout>

        </LinearLayout>

        <Button
            android:id="@+id/teleop_continue"
            android:layout_width="150dp"
            android:layout_height="60dp"
            android:layout_below="@id/teleop_layout_main"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="@dimen/activity_horizontal_margin"
            android:layout_marginTop="@dimen/activity_horizontal_margin"
            android:layout_marginEnd="@dimen/activity_horizontal_margin"
            android:layout_marginBottom="@dimen/activity_horizontal_margin"
            android:elevation="6dp"
            android:text="@string/continue_button"
            android:textColor="#ffffff"
            tools:ignore="ExtraText" />

        <TextView
            android:id="@+id/teleop_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:text="Tele-operated"
            android:textSize="40dp"></TextView>

    </RelativeLayout>


</ScrollView>
